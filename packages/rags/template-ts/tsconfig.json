{
    "compilerOptions": {
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "target": "ESNext",
        "module": "ESNext",
        "lib": [
            "ESNext",
            "DOM",
        ],
        "types": [
            "node"
        ],
        "strict": true,
        "resolveJsonModule": true,
        "noImplicitReturns": true,
        "isolatedModules": false,
        "skipLibCheck": true,
    },
}