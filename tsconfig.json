{
    "compilerOptions": {
        "moduleResolution": "nodenext",
        "target": "ESNext",
        "module": "ESNext",
        "lib": [
            "ESNext",
            "DOM"
        ],
        "strict": true,
        "resolveJsonModule": true,
        "noImplicitReturns": true,
        "isolatedModules": false,
        "skipLibCheck": true,
    },
}